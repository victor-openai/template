#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/victor/chatkit/template/node_modules/.pnpm/typedoc@0.28.13_typescript@5.8.3/node_modules/typedoc/bin/node_modules:/Users/victor/chatkit/template/node_modules/.pnpm/typedoc@0.28.13_typescript@5.8.3/node_modules/typedoc/node_modules:/Users/victor/chatkit/template/node_modules/.pnpm/typedoc@0.28.13_typescript@5.8.3/node_modules:/Users/victor/chatkit/template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/victor/chatkit/template/node_modules/.pnpm/typedoc@0.28.13_typescript@5.8.3/node_modules/typedoc/bin/node_modules:/Users/victor/chatkit/template/node_modules/.pnpm/typedoc@0.28.13_typescript@5.8.3/node_modules/typedoc/node_modules:/Users/victor/chatkit/template/node_modules/.pnpm/typedoc@0.28.13_typescript@5.8.3/node_modules:/Users/victor/chatkit/template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typedoc/bin/typedoc" "$@"
else
  exec node  "$basedir/../typedoc/bin/typedoc" "$@"
fi
